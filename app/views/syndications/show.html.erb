<%= render 'layouts/jumbtron' %>
<section class="main-body without-search-feature">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <div class="box-shadow">
          <ol class="breadcrumb">
            <li><%= link_to 'Home',root_url %></li>
            <li><%= link_to 'Investor Performance', syndications_path %></li>
            <li class="active"><%= @syndication.title %></li>
            <% if current_user %>
            <li><%= link_to 'Edit', edit_syndication_path(@syndication) %></li>
            <% end %>
          </ol>
        </div>
      </div>
    </div>
    <%= @syndication.title %><br>
    <%= image_tag @syndication.image.thumb.url %><br>
    <div class="row">
      <div class="col-xs-12">
        <% @syndication.annual_returns.each do |annual_return| %>
        <%= annual_return.year %><br>
        <%= annual_return.projected_annual_return %><br>
        <%= annual_return.actual_annual_return %><br>
        <%= annual_return.quarter_1 %><br>
        <%= annual_return.quarter_2 %><br>
        <%= annual_return.quarter_3 %><br>
        <%= annual_return.quarter_4 %><br>
        <% end %>
      </div>
    </div>
  </div>
</section>