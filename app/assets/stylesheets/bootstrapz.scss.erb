.white {
    color: white !important;
}
.gold {
    color: rgb(226,184,91) !important;
}
.blue {
    color: rgb(13,104,159) !important;
}
.text-danger {
    color: #D83D3A !important;
}


.page-header {
    margin-bottom: 80px;
}
.wipalert {
    display: block;
    background-color: #e25050;
    color: white;
    padding: 12px;
    border-radius: 4px;
    font-size: 22px;
    text-decoration: none;
}
.goldalert {
    display: block;
    background-color: rgb(226,184,91);
    color: white;
    padding: 12px;
    border-radius: 4px;
    font-size: 22px;
    text-decoration: none;
}
a.wipalert:hover {
    color: white;
    background-color: rgb(255,79,76);
    text-decoration: none;
}
.ratio16x9 {
  width: 50%;
  /* whatever width you want */
  display: inline-block;
  position: relative;
}
.block {
  display: block;
}
.table.no-top-border > tbody > tr > td {
  border-top: none;
}
.btn.disabled.no-fade, .btn[disabled].no-fade, fieldset[disabled].no-fade .btn.no-fade {
    opacity: 1.0;
}
.modal-content .panel {
    margin-bottom: 0;
}
.barcontainer {
    background-color: #e2b85b;
    padding-top: 8px;
    padding-bottom: 8px;
}
.breadcrumb {
    margin-top: 20px;
}
