}

body { 
     padding-top: 0px; 
     padding-bottom: 40px;
     font-family: AllerRG;
     color: #525252;
}

.container-full {
  margin: 0 auto;
  width: 100%;
}

.top-nav-bg {
    background-color: #407cbb;
}

.gold_bar {
    height: 4px;
    /*background-color: rgb(154,128,70);*/
